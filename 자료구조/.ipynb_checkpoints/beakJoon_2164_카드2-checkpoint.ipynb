   "source": [
    "from collections import deque\n",
    "\n",
    "n = int(input())\n",
    "dq = deque([i+1 for i in range(n)])\n",
    "\n",
    "while len(dq) > 1:\n",
    "    dq.popleft()\n",
    "    dq.rotate(-1)\n",
    "    print(dq)\n",
    "    \n",
    "print(*dq)"
   ]
