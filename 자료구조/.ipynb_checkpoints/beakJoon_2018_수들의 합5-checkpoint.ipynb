   "source": [
    "n = int(input())\n",
    "\n",
    "start,end,sum,cnt = 1,1,1,1\n",
    "\n",
    "while(end != n):\n",
    "    if sum == n:\n",
    "        cnt+=1\n",
    "        end += 1\n",
    "        sum += end\n",
    "    elif sum > n:\n",
    "        sum -= start\n",
    "        start += 1\n",
    "    else:\n",
    "        end += 1\n",
    "        sum += end\n",
    "\n",
    "print(cnt)"
   ]
  }
