   "source": [
    "n = int(input())\n",
    "\n",
    "A = [0]*n\n",
    "\n",
    "for i in range(n):\n",
    "    A[i] = int(input())\n",
    "    \n",
    "for i in range(n):\n",
    "    for j in range(n-1-i):\n",
    "        if A[j] > A[j+1]:\n",
    "            temp = A[j]\n",
    "            A[j] = A[j+1]\n",
    "            A[j+1] = temp\n",
    "\n",
    "for i in range(n):\n",
    "    print(A[i])"
   ]
  }
